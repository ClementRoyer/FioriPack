{
  "version": "1.0.8",
  "name": "fiori-pack",
  "displayName": "Fiori Pack / Business Application Studio Pack",
  "description": "Popular extensions for Fiori / SAPUI5 development and Business Application Studio (SAP BAS) that provide IntelliSense, Git, ESLint, Error Lens and many other useful tools",
  "publisher": "clement-royer",
  "icon": "images/package_icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClementRoyer/FioriPack"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Extension Packs"
  ],
  "extensionPack": [
    "saposs.app-studio-remote-access",
    "saposs.app-studio-toolkit",
    "saposs.sap-guided-answers-extension",
    "saposs.vscode-ui5-language-assistant",
    "saposs.xml-toolkit",
    "sapse.sap-ux-annotation-modeler-extension",
    "sapse.sap-ux-application-modeler-extension",
    "sapse.sap-ux-fiori-tools-extension-pack",
    "sapse.sap-ux-help-extension",
    "sapse.sap-ux-service-modeler-extension",
    "sapse.vsc-extension-odata-csdl-modeler",
    "iljapostnovs.ui5plugin",
    "dbaeumer.vscode-eslint",
    "christian-kohler.npm-intellisense",
    "christian-kohler.path-intellisense",
    "larshp.vscode-abap",
    "larshp.vscode-abaplint",
    "dotjoshjohnson.xml",
    "formulahendry.auto-complete-tag",
    "tomoki1207.pdf",
    "leo-ls.fiori-xml-lint",
    "ms-vscode-remote.remote-ssh",
    "ms-vscode-remote.remote-ssh-edit",
    "ms-vscode.remote-explorer",
    "davidanson.vscode-markdownlint",
    "seatonjiang.gitmoji-vscode",
    "usernamehw.errorlens",
    "aaron-bond.better-comments"
  ],
  "scripts": {
    "package": "vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.3.2"
  }
}
